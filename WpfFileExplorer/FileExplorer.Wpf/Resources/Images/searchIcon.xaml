<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--This file is NOT compatible with Silverlight-->
    <Viewbox x:Key="SearchIcon" Stretch="UniformToFill" x:Shared="False">
        <Grid Height="30" Width="20">
            <Ellipse Width="18" Height="18" Name="circle3385" StrokeThickness="1" Stroke="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=Control}, Path=Foreground}" StrokeMiterLimit="10" />
            <Line X1="4" Y1="28" X2="13" Y2="19" Name="line3387" StrokeThickness="1" Stroke="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=Control}, Path=Foreground}" StrokeMiterLimit="10" />
        </Grid>
    </Viewbox>
</ResourceDictionary>
